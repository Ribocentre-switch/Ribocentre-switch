  <script type="text/javascript">
    function showImage0(imageUrl) {
      var imagePreview = document.getElementById("image-preview0");
      var imageLink = document.getElementById("image-link0");
      var imageCaption = document.getElementById("image-caption");

      imagePreview.src = imageUrl;
      imageLink.href = imageUrl;

      if (imageUrl === "/images/2D/THF-I_riboswitch_2D1.svg") {
        imageCaption.innerHTML = "This is image a";
      } else if (imageUrl === "/images/2D/THF-I_riboswitch_2D2.svg") {
        imageCaption.innerHTML = "This is image b";
      }
    }

    function openImage(element) {
      var imageUrl = element.getAttribute("href");
      window.open(imageUrl, "_blank");
      return false;
    }

    // Initialize zoom.js on all images with the "zoomable" class
    zoomjs.init({
      selector: ".zoomable",
    });
  </script>
